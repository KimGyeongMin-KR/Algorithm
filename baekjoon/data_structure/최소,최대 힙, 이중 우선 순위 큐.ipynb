   "source": [
    "import sys\n",
    "import heapq\n",
    "input = sys.stdin.readline\n",
    "\n",
    "min_heap = []\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    if not n:\n",
    "        print(heapq.heappop(min_heap) if min_heap else 0)\n",
    "    else:\n",
    "        heapq.heappush(min_heap, n)"
   ]
